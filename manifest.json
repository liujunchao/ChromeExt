{
    "version":"1.0",
    "manifest_version":2,
    "name":"chrome api practice",
    "description":"demo project to practice chrome api",
    "icons":{
        "16":"images/icon16.png" 
    },
    "browser_action":{
        "default_icon":{
            "19":"images/icon19.png"
        },
        "default_title":"chrome api",
        "default_popup":"popup.html"
    },
    "background":{
        "scripts":[
            "scripts/background.js"
        ]
    },
    "content_scripts":[
      {
         "matches": ["*://*/*"],
         "js": ["js/content.js"]
      }  
    ],
    "permissions":[
        "contextMenus"
    ]
}